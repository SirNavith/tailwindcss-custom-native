    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest-matcher-css": "^1.1.0",
    "mocha": "^8.1.1",
    "postcss": "^7.0.32",
    "tailwindcss-pseudo": "^1.0.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
