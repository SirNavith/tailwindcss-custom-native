  "description": "Write your Tailwind CSS configuration file with your own custom utilities as though they were native to the framework",
  "author": "Jacob Babich <jacobbabichpublic+git@gmail.com>",
  "keywords": [
    "plugin",
    "tailwind",
    "tailwindcss",
    "tailwindcss-plugin",
    "custom-utilities",
    "custom-native"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/babichjacob/tailwindcss-custom-native.git"
  },
  "bugs": "https://github.com/babichjacob/tailwindcss-custom-native/issues",
  "homepage": "https://github.com/babichjacob/tailwindcss-custom-native",
  "files": [
    "dist/src/*"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
